public class Exercicio1 {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        int num = 1;
        
        while(num <= 10){
       if(num == 1){
            System.out.println(num + " elefante incomoda mt gente ");
        num++;
       }
       if(num == 2){
         System.out.println(num + " elefantes incomodam incomodam mt mais ");
        num++;
       }
        }
    }
    
}
